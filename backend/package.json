  "description": "Vue project",
  "main": "index.js",
  "scripts": {
    "generate-docs": "jsdoc -c ./jsdoc.json",
    "build:dll": "cross-env NODE_ENV=production webpack --hide-modules --config ./w-build/dll.config.js ",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules --config ./w-build/webpack.dev.config.js",
    "build:prd": "cross-env NODE_ENV=production webpack --hide-modules --config ./w-build/webpack.prd.config.js",
    "build": "npm run build:prd",
    "watch:dev": "cross-env NODE_ENV=development webpack  --watch --hide-modules  --config ./w-build/webpack.dev.config.js",
    "watch:prd": "cross-env NODE_ENV=production webpack --watch --hide-modules --config ./w-build/webpack.prd.config.js",
    "watch": "npm run watch:dev",
    "server": "webpack-dev-server --config ./w-build/webpack.server.config.js",
    "start": "npm run server"
  },
